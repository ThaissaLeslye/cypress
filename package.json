{
  "name": "cypress-playground",
  "scripts": {
    "test": "npx cypress run --reporter mochawesome --reporter-options overwrite=false,html=false,json=true",
    "merge-reports": "npx mochawesome-merge cypress/reports/*.json > mochareport.json",
    "generate-report": "npx marge mochareport.json --reportDir mochawesome-report"
  },
  "devDependencies": {
    "cypress": "^14.5.4",
    "cypress-mochawesome-reporter": "^4.0.2",
    "mochawesome": "^7.1.3"
  }
}
